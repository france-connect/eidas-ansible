<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <comment>SWModule encrypt with JKS.</comment>
    <entry key="check_certificate_validity_period">false</entry>
    <entry key="disallow_self_signed_certificate">false</entry>
    <entry key="response.encryption.mandatory">false</entry>
    <!-- Data Encryption algorithm -->
    <entry key="data.encryption.algorithm">http://www.w3.org/2009/xmlenc11#aes256-gcm</entry>
    <!-- Decryption algorithm Whitelist-->
    <entry key="encryption.algorithm.whitelist">
        http://www.w3.org/2009/xmlenc11#aes128-gcm;
        http://www.w3.org/2009/xmlenc11#aes256-gcm;
        http://www.w3.org/2009/xmlenc11#aes192-gcm
    </entry>
    <!-- Key Encryption algorithm -->
    <entry key="key.encryption.algorithm">http://www.w3.org/2001/04/xmlenc#rsa-oaep-mgf1p</entry>

    <entry key="keyStorePassword">{{ connector_encryption_communication_keystore_password }}</entry>
    <entry key="keyPassword">{{ connector_encryption_communication_key_password }}</entry>
    <entry key="keyStoreType">{{ connector_encryption_communication_keystore_type }}</entry>
    
    <entry key="keyStorePath">{{ keystores_directory }}{{ connector_encryption_communication_keystore_file_name }}</entry>

    <!-- Management of the encryption activation -->
    <entry key="encryptionActivation">{{ config_directory }}encryptionConf.xml</entry>

    <!--  If not present then no encryption will be applied on response -->
    <entry key="responseToPointIssuer.CA">{{ connector_encryption_communication_certificate_issuer_ca }}</entry>
    <entry key="responseToPointSerialNumber.CA">{{ connector_encryption_communication_certificate_serialnumber_ca }}</entry>

    <!--  If not present then no decryption will be applied on response -->
    <entry key="responseDecryptionIssuer">{{ connector_decryption_communication_certificate_issuer }}</entry>
    <entry key="serialNumber">{{ connector_decryption_communication_certificate_serialnumber }}</entry>

</properties>
